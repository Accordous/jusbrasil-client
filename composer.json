{
    "name": "accordous/jusbrasil-client",
    "description": "Client for JusBrasil API",
    "type": "library",
    "authors": [
        {
            "name": "Pedro Augusto",
            "email": "pams.pedro@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Accordous\\JusBrasilClient\\Providers\\JusBrasilClientServiceProvider"
            ],
            "aliases": {
              "JusBrasilClient" : "Accordous\\JusBrasilClient\\Facades\\JusBrasilClient"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Accordous\\JusBrasilClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accordous\\JusBrasilClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^10",
        "illuminate/http": "^10.0",
        "guzzlehttp/guzzle": "^7.9",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5"
    }
}
